body
	background-color #C30FEA
section
	border 10px solid #3B2424
	background-color #fff
	height 300px
	margin 40px auto
	text-align center
	width 400px
section.pointer 
	cursor pointer
section.move
	cursor move
section.wait
	cursor wait
section.help
	cursor help 
section.image
	cursor url(../imgs/pokeball.png), default